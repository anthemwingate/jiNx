
# .coveragerc to control coverage.py
[run]
branch = True
source = DiTTo_YoutubePredictor

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
omit =
    */DiTTo_YoutubePredictor/

[html]
directory = coverage_html_report